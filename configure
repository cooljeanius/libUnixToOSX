    "poll/poll-emulator/Makefile") CONFIG_FILES="$CONFIG_FILES poll/poll-emulator/Makefile" ;;
    "poll/poll-emulator/Makefile_nonAutomake") CONFIG_FILES="$CONFIG_FILES poll/poll-emulator/Makefile_nonAutomake" ;;

  *) as_fn_error $? "invalid argument: '$ac_config_target'" "$LINENO" 5;;
  esac
