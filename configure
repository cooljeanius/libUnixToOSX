int
main (void)
{

  int result = 0;
  char *fence = NULL;
#if (defined(HAVE_SYS_MMAN_H) && HAVE_SYS_MMAN_H) && HAVE_MPROTECT
# if defined(HAVE_MAP_ANONYMOUS) && HAVE_MAP_ANONYMOUS
  const int flags = (MAP_ANONYMOUS | MAP_PRIVATE);
  const int fd = -1;
# else /* !HAVE_MAP_ANONYMOUS: */
